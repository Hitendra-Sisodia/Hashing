// counting frequency of characters elements
#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s = "abcdefaaa";
    int hash[26] = {0};
    for(int i = 0 ; i < s.size() ; i++){
        hash[s[i] - 'a']++;
    }
    char ch;
    cout<<"Enter a character to count frequency: ";
    cin>>ch;
    cout<<hash[ch - 'a'];
}
